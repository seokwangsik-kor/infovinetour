<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="6sp"
    android:layout_marginLeft="5sp"
    android:layout_marginRight="5sp"
    android:gravity="center">

    <!--<LinearLayout
        android:id="@+id/list_type_a_layer"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:orientation="vertical">-->

        <FrameLayout
            android:id="@+id/list_type_a_btn_layer"
            android:layout_width="match_parent"
            android:layout_height="30dp">

            <CheckBox
                android:id="@+id/list_type_a_check"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:background="@drawable/list_type_a_btn"
                android:button="@android:color/transparent"
                android:clickable="false" />

            <TextView
                android:id="@+id/list_type_a_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/s_core_dream_4"
                android:text=""
                android:textColor="#4f5664"
                android:textSize="14sp"
                android:textStyle="normal" />

        </FrameLayout>
    <!--</LinearLayout>-->
</LinearLayout>